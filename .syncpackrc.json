{
  "source": ["package.json", "packages/*/package.json"],
  "semverGroups": [
    {
      "packages": ["**"],
      "dependencies": ["**"],
      "dependencyTypes": ["dependencies", "devDependencies", "peerDependencies"],
      "policy": "sameRange"
    }
  ],
  "versionGroups": [
    {
      "packages": ["packages/*"],
      "dependencies": ["onsocial-*"],
      "policy": "ignore"
    }
  ]
}
