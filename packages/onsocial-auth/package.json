{
  "name": "onsocial-auth",
  "version": "0.1.12",
  "description": "Expo-compatible SDK for OnSocial wallet authentication using HERE Wallet and JWT.",
  "author": "OnSocial Labs",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "onsocial",
    "wallet",
    "here-wallet",
    "jwt",
    "expo",
    "auth"
  ],
  "scripts": {
    "build": "pnpm exec tsc",
    "clean": "rm -rf dist",
    "lint": "eslint ./src --ext .ts,.tsx,.js,.jsx",
    "test": "vitest run",
    "format": "prettier --write .",
    "tsc": "pnpm exec tsc --noEmit"
  },
  "dependencies": {
    "@here-wallet/core": "^3.4.0",
    "axios": "^1.10.0",
    "expo-secure-store": "^14.2.3",
    "react": "^19.1.0",
    "react-native": "^0.80.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "eslint": "^9.30.1",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "@eslint/js": "^9.30.1"
  }
}
