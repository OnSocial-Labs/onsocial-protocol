{
  "name": "onsocial-js",
  "version": "0.1.0",
  "description": "Expo-safe NEAR transaction utilities for OnSocial frontend integration.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf dist",
    "lint": "eslint ./src --ext .ts,.tsx,.js,.jsx",
    "test": "vitest run",
    "format": "prettier --write .",
    "tsc": "tsc"
  },
  "keywords": [
    "near",
    "expo",
    "react-native",
    "transactions",
    "onsocial"
  ],
  "author": "OnSocial Labs",
  "license": "MIT",
  "dependencies": {
    "@near-js/transactions": "^2.1.0",
    "@near-js/types": "^2.0.3",
    "@near-js/utils": "^2.1.0",
    "base64-js": "^1.5.1",
    "borsh": "^0.7.0",
    "bs58": "^5.0.0",
    "cross-fetch": "^4.1.0",
    "js-sha256": "^0.11.1",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "eslint-plugin-prettier": "^5.5.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
